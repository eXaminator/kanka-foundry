{{#>../shared/overviewLayout}}
    {{#*inline 'attributes'~}}
        {{~>../shared/detailRow value=data.system.snapshot.type label='shared.labels.type' }}
        {{~>../shared/detailRowArray references=data.system.snapshot.ancestors label='shared.labels.parent' }}
        {{~>../shared/detailRow value=data.system.snapshot.location_id type='location' label='shared.labels.location' reference=true }}

        {{~#with (kankaFilterBy (kankaFilterReferences data.system.snapshot.members) 'pin_id' 'regex:2|3') as | starred |~}}
            {{~#each starred ~}}
                {{~>../shared/detailRow value=character_id type='character' rawLabel=role label='organisation.labels.member' reference=true isSecret=(kankaIsSecret this) }}
            {{~/each~}}
        {{~/with~}}
    {{~/inline}}
{{/../shared/overviewLayout}}
